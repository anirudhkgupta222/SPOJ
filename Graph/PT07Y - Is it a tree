#include <bits/stdc++.h>
using namespace std;

void dfs(int node,vector<int> &vis,vector<vector<int>> &ar){
    
    vis[node] = 1;
    
    for(int i = 0; i < ar[node].size(); i++){
        
        if(vis[ar[node][i]] == 0){
            dfs(ar[node][i],vis,ar);
        }
    }
}
int main() {
    
    int n,m;
    cin >> n >> m;
    
    if(m != n -1){
        cout << "NO" << endl;
    }
    else{
        vector<vector<int>> ar(n+1,vector<int>());
        
        while(m--){
            int a,b;
            cin  >> a >> b;
            ar[a].push_back(b);
            ar[b].push_back(a);
        }
        
        vector<int> vis(n+1,0);
        int cc_count = 0;
        for(int i = 1; i <= n; i++){
            if(vis[i] == 0){
                dfs(i,vis,ar);
                cc_count++;
            }
        }
        if(cc_count == 1){
            cout << "YES" << endl;
        }
        else{
            cout << "NO" << endl;
        }
    }
	return 0;
}
